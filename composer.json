{
    "name": "fi/pannelloamministrazionebundle",
    "description": "Pannello di amministrazione per progetti Fifree2",
    "type": "symfony-bundle",
    "authors": [{
            "name": "Andrea Manzi",
            "email": "andrea.manzi@comune.fi.it"
        }],
    "keywords": [
        "Pannello amministrazione bundle"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fi\\PannelloAmministrazioneBundle\\": "src",
            "Fi\\ProvaBundle\\": "src/Tests/src/Fi/ProvaBundle"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "fi/osbundle": "~2.0",
        "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": "^3.0",
        "mysql-workbench-schema-exporter/doctrine2-exporter": "^3.0"
    },
    "require-dev": {
        "symfony/symfony": "2.8.*",
        "sensiolabs/security-checker": "^3.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.4",
        "sebastian/phpcpd": "^2.0",
        "mockery/mockery": "^0.9.5",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/inflector": "1.0.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "symfony/yaml": "2.8.*",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "sensio/generator-bundle": "~2.0",
        "behat/behat": "3.0.*",
        "behat/mink-selenium2-driver": "1.2.*",
        "behat/symfony2-extension": "2.0.*",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "~1.1",
        "se/selenium-server-standalone": "^2.53",
        "phpunit/phpunit": "^5.5",
        "fi/fifreecorebundle": "~2.0",
        "friendsofsymfony/user-bundle": "dev-master#47e4de9d8b6fed521729fb38107d9413863fe4ce as 2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}