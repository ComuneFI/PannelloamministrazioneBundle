{
    "name": "fi/pannelloamministrazionebundle",
    "description": "Pannello di amministrazione per progetti Fifree2",
    "type": "symfony-bundle",
    "authors": [{
            "name": "Andrea Manzi",
            "email": "andrea.manzi@comune.fi.it"
        }],
    "keywords": [
        "Pannello amministrazione bundle"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fi\\PannelloAmministrazioneBundle\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.comune.intranet"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "fi/osbundle": "~1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.4",
        "phpmd/phpmd": "~1.5",
        "sensiolabs/security-checker": "~1.1",
        "phpunit/phpunit": "4.8.21",
        "satooshi/php-coveralls": "~0.6.1",
        "sebastian/phpcpd": "~1.4",
        "mockery/mockery": "~0.9"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
