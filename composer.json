{
    "name": "fi/pannelloamministrazionebundle",
    "description": "Pannello di amministrazione per progetti Fifree2",
    "type": "symfony-bundle",
    "authors": [{
            "name": "Andrea Manzi",
            "email": "andrea.manzi@comune.fi.it"
        }],
    "keywords": [
        "Pannello amministrazione bundle"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fi\\PannelloAmministrazioneBundle\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.comune.intranet"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "fi/osbundle": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "sensiolabs/security-checker": "^3.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.4",
        "sebastian/phpcpd": "^2.0",
        "mockery/mockery": "^0.9.5",
        "symfony/symfony": "2.8.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
